<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="D9D9D9"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view4"
        android:layout_width="226dp"
        android:layout_height="43dp"
        android:background="@drawable/chat_bubble"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="134dp" />

    <View
        android:id="@+id/view5"
        android:layout_width="158dp"
        android:layout_height="43dp"
        android:background="@drawable/chat_bubble"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="193dp" />

    <View
        android:id="@+id/view7"
        android:layout_width="123dp"
        android:layout_height="43dp"
        android:background="@drawable/chat_bubble"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="261dp" />

    <View
        android:id="@+id/view8"
        android:layout_width="188dp"
        android:layout_height="98dp"
        android:background="@drawable/chat_bubble"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="317dp" />

    <View
        android:id="@+id/view9"
        android:layout_width="329dp"
        android:layout_height="50dp"
        android:background="@drawable/chat_bubble"
        android:backgroundTint="#19578F"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="725dp" />

    <View
        android:id="@+id/view2"
        android:layout_width="360dp"
        android:layout_height="92dp"
        android:background="#195790"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-3dp"
        tools:ignore="MissingConstraints,VisualLintBounds" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="Yura"
        android:textColor="#D9D9D9"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="33dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="79dp"
        android:layout_height="79dp"
        app:srcCompat="@drawable/photo_profile"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="6dp"
        tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="Permisi, saya mau tanya"
        android:textColor="#D9D9D9"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view4"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="Iya Silahkan"
        android:textColor="#D9D9D9"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/view5"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="169dp"
        android:layout_height="51dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Berminat menjadi siswa saya?"
        android:textColor="#D9D9D9"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap,TextSizeCheck"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="324dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="23dp"
        android:layout_height="22dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Ya"
        android:textColor="#AED581"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap,TextSizeCheck"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="375dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="42dp"
        android:layout_height="21dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Tidak"
        android:textColor="#E53935"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap,TextSizeCheck,TextContrastCheck"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="376dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="Oke"
        android:textColor="#D9D9D9"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintOverlap" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="45dp"
        android:layout_height="47dp"
        android:background="@drawable/back_butt"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp"
        android:importantForAccessibility="no" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="45dp"
        android:layout_height="47dp"
        android:background="@drawable/key_butt"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/view9"
        app:layout_constraintStart_toStartOf="@+id/view9"
        app:layout_constraintTop_toTopOf="@+id/view9"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="45dp"
        android:layout_height="47dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/send_butt"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/view9"
        app:layout_constraintEnd_toEndOf="@+id/view9"
        app:layout_constraintTop_toTopOf="@+id/view9"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>